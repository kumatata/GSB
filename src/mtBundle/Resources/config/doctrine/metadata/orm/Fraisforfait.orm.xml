<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Fraisforfait" table="fraisforfait">
    <indexes>
      <index name="idFraisForfait" columns="idFraisForfait"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idfraisforfait" type="string" column="idFraisForfait" length="3" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="libelle" type="string" column="libelle" length="20" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="montant" type="decimal" column="montant" precision="5" scale="2" nullable="true"/>
  </entity>
</doctrine-mapping>
